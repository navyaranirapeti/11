   "source": [
    "## Recursion power of num\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "1024"
      ]
     },
     "execution_count": 4,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "def rc(n,r):\n",
    "    if r==1:\n",
    "        return n\n",
    "    else:\n",
    "        return rc(n,r-1)*n\n",
    "rc(2,10)"
   ]
  },
  {
